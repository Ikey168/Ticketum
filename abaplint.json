{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "dependencies": [],
  "syntax": {
    "version": "v754",
    "errorNamespace": "^(Z|Y)",
    "globalConstants": [],
    "globalMacros": []
  },
  "rules": {
    "abapdoc": false,
    "allowed_object_naming": true,
    "allowed_object_types": true,
    "ambiguous_statement": true,
    "avoid_use": true,
    "begin_end_names": true,
    "chain_mainly_declarations": true,
    "check_comments": false,
    "check_ddic": true,
    "check_include": true,
    "check_syntax": true,
    "check_text_elements": true,
    "class_attribute_names": true,
    "cloud_types": true,
    "colon_missing_space": true,
    "commented_code": true,
    "constructor_visibility_public": true,
    "contains_tab": true,
    "definitions_top": true,
    "description_empty": true,
    "double_space": true,
    "empty_line_in_statement": true,
    "empty_statement": true,
    "empty_structure": true,
    "exit_or_check": true,
    "exporting": true,
    "form_tables_obsolete": true,
    "fully_type_constants": true,
    "functional_writing": true,
    "global_class": true,
    "identical_conditions": true,
    "identical_contents": true,
    "identical_form_names": true,
    "if_in_if": true,
    "implement_methods": true,
    "in_statement_indentation": false,
    "indentation": true,
    "inline_data_old_versions": true,
    "keep_single_parameter_on_one_line": true,
    "keyword_case": {
      "style": "upper",
      "ignoreExceptions": true,
      "ignoreLowerClassImplmentation": true,
      "ignoreGlobalClassDefinition": false,
      "ignoreGlobalInterface": false,
      "ignoreFunctionModuleName": false
    },
    "line_break_multiple_parameters": true,
    "line_length": {
      "length": 120
    },
    "line_only_punc": true,
    "local_class_naming": true,
    "local_testclass_location": true,
    "local_variable_names": true,
    "main_file_contents": true,
    "max_one_statement": true,
    "message_exists": true,
    "method_length": {
      "statements": 100,
      "errorWhenEmpty": true,
      "ignoreTestClasses": false
    },
    "method_parameter_names": true,
    "mix_returning": true,
    "msag_consistency": true,
    "nesting": true,
    "newline_between_methods": true,
    "no_public_attributes": true,
    "object_naming": true,
    "obsolete_statement": true,
    "parser_error": true,
    "prefer_inline": true,
    "prefer_returning_to_exporting": true,
    "preferred_compare_operator": true,
    "prefix_is_current_class": true,
    "release_idoc": true,
    "remove_descriptions": true,
    "rfc_error_handling": true,
    "sequential_blank": true,
    "short_case": true,
    "sicf_consistency": true,
    "space_before_colon": true,
    "space_before_dot": true,
    "sql_escape_host_variables": true,
    "start_at_tab": true,
    "superclass_final": true,
    "tabl_enhancement_category": true,
    "try_without_catch": true,
    "type_form_parameters": true,
    "types_naming": true,
    "unknown_types": true,
    "unreachable_code": true,
    "unused_variables": true,
    "use_new": true,
    "when_others_last": true,
    "whitespace_end": true,
    "xml_consistency": true
  }
}
